pipeline {
    agent any

    stages {
        stage('compile') {
            steps {
                script{
                    echo "compile the code"
                }

                }
            }
        
    
    
        stage('codeReview') {
            steps {
                script{
                    echo "codereview  the code"
                }

                }
            }
        
        stage('unitTest') {
            steps {
                script{
                    echo "test  the code"
                }

                }
            
        }
        
        
    
}
}
